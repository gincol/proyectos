<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="es.bcn.imi.framework.vigia.frontal.gap.inventary.orm.dao.GroupFurnitureGapDao">
   
    <select id="getGroupsFurnitureGap" parameterType="java.util.Map" resultType="es.bcn.vigia.fmw.libcommons.integration.orm.model.gap.GroupFurnitureGap">
		SELECT 
	        CODI AS codeGroup, 
			DESCRIPCIO AS descriptionGroup,
			TIPUS_CODI AS codeType, 
			TIPUS_DESCRIPCIO AS descriptionType
		FROM 
			VW_DM_AGRUP_TIPUS_MOBILIARI
		WHERE 
			CODI = #{codeGroup}
	</select>
    
</mapper>
