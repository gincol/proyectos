<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="es.bcn.imi.framework.vigia.frontal.orm.dao.ContractDao">

	<select id="getContract" resultType="es.bcn.vigia.fmw.libcommons.integration.orm.model.Contract">
		SELECT
			CODI AS code,
			CLIENT_ID AS clientId,
			ROL AS rol
		FROM
			TB_CONTRACTA
		WHERE
			CLIENT_ID = #{clientId}
	</select>

</mapper>